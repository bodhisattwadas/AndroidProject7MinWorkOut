<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray_black"
    tools:context=".Settings" >

<RelativeLayout
    android:id="@+id/relativeLayout1"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_alignParentLeft="true"
    android:layout_alignParentTop="true"
    android:background="@color/deep_green_header_footer"
    android:gravity="center"
    android:padding="5dp" >

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:src="@drawable/normal_question_mark" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="14dp"
        android:layout_toLeftOf="@+id/imageView1"
        android:src="@drawable/normal_lock" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:text="SETTINGS"
        android:textColor="#fff"
        android:textSize="25sp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView3"
        android:layout_alignParentLeft="true"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:src="@drawable/normal_back" />
</RelativeLayout>

<LinearLayout
    android:id="@+id/linearLayout1"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_alignParentBottom="true"
    android:layout_alignParentLeft="true"
    android:background="@color/deep_green_header_footer"
    android:gravity="center"
    android:weightSum="1" >

    <ImageButton
        android:id="@+id/imageButton1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/home"
        android:layout_weight="0.25" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/learn"
        android:layout_weight="0.25" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/logs"
        android:layout_weight="0.25" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/settings"
        android:layout_weight="0.25" />
        
    </LinearLayout>

<TableLayout
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_below="@+id/relativeLayout1"
    android:layout_centerHorizontal="true" >

    <TableRow
        android:id="@+id/tableRow1"
        style="@style/settings_table_row_style"
        android:gravity="left" >

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Voice Over"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#FFF" />
        
    </TableRow>
    <View
    	android:layout_width="fill_parent"
    	android:layout_height="1dp"
    	android:background="#fff"/>

    <TableRow
        android:id="@+id/tableRow2"
        style="@style/settings_table_row_style" >

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="20dp"
            android:text="Reminders"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#FFF" />


    </TableRow>
    <View
    	android:layout_width="fill_parent"
    	android:layout_height="1dp"
    	android:background="#fff"/>

    <TableRow
        android:id="@+id/tableRow3"
        style="@style/settings_table_row_style"
        android:layout_gravity="center" >

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Exercise Time"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#FFF" />

    </TableRow>
    <View
    	android:layout_width="fill_parent"
    	android:layout_height="1dp"
    	android:background="#fff"/>

    <TableRow
        android:id="@+id/tableRow4"
        style="@style/settings_table_row_style" >
        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Rest Time"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#FFF" />
    </TableRow>
    <View
    	android:layout_width="fill_parent"
    	android:layout_height="1dp"
    	android:background="#fff"/>
    <TableRow
        android:id="@+id/tableRow5"
        style="@style/settings_table_row_style" >
        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Set Number"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#FFF" />
    </TableRow>
    <View
    	android:layout_width="fill_parent"
    	android:layout_height="1dp"
    	android:background="#fff"/>
    <TableRow
        android:id="@+id/tableRow6"
        style="@style/settings_table_row_style" >
        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Volume level"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#FFF" />
    </TableRow>
    <View
    	android:layout_width="fill_parent"
    	android:layout_height="1dp"
    	android:background="#fff"/>
</TableLayout>

</RelativeLayout>